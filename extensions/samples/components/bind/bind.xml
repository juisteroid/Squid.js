<?xml version="1.0" encoding="utf-8" ?>
<div>
    <div sq-context="details" sq-bind="data" >
        <label name="nm">FullName:</label>
        <div>
            <input name="nm" value="${name}"/>
            <input name="lstnm" value="${lastname}"/>
        </div>
        <label name="contacts">Contact Numbers:</label>
        <div sq-context="contacts">
        <label name="home">Home:</label>
            <input name="home" value="${home}"/>
            <label name="mobile">Mobile:</label>
            <input name="mobile" value="${mobile}"/>
        </div>
        <label name="addrs">Address:</label>
        <input name="addrs" value="${address}"/>
    </div>
